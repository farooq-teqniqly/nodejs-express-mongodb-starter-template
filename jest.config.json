{
  "testEnvironment": "node",
  "verbose": true,
  "transform": {
    "^.+\\.js?$": "babel-jest"
  },
  "transformIgnorePatterns": ["/node_modules/"],
  "globalSetup": "<rootDir>/globalSetup.js",
  "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
  "globalTeardown": "<rootDir>/globalTeardown.js"
}
